# Server Configuration
PORT=5000
NODE_ENV=development # Change to 'production' when deploying

# MongoDB Connection
MONGO_URI=mongodb+srv://your-mongodb-connection-string

# JWT Secret
JWT_SECRET=your-jwt-secret-key

# Google OAuth
YOUR_GOOGLE_CLIENT_ID=your-google-client-id
YOUR_GOOGLE_CLIENT_SECRET=your-google-client-secret

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your-cloud-name
CLOUDINARY_API_KEY=your-api-key
CLOUDINARY_API_SECRET=your-api-secret

# Stripe Configuration (if using real Stripe)
STRIPE_SECRET_KEY=your-stripe-secret-key

# Deployment URLs (for production)
# BACKEND_URL=https://your-deployed-backend-url.com
# FRONTEND_URL=https://your-deployed-frontend-url.com
